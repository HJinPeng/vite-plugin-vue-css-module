{
  "name": "vite-plugin-vue-css-module",
  "version": "2.1.0",
  "description": "css-module syntactic sugar for vue3",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "format": "prettier --write src/",
    "release": "npm version patch && npm publish",
    "test": "vitest",
    "format:test": "prettier --write test/"
  },
  "keywords": [
    "css-module",
    "cssModule",
    "css module",
    "css-modules",
    "cssModules",
    "css modules",
    "class",
    "cls",
    "vue3",
    "vite",
    "pug"
  ],
  "author": "jinpengh <13143352767@163.com>",
  "contributors": [
    {
      "name": "Ilya Semenov",
      "email": "ilya@semenov.co",
      "url": "https://github.com/IlyaSemenov"
    },
    {
      "name": "Gideon Shi",
      "email": "sjym90@gmail.com",
      "url": "https://github.com/shijinyu"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HJinPeng/vite-plugin-vue-css-module.git"
  },
  "homepage": "https://github.com/HJinPeng/vite-plugin-vue-css-module#readme",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.44.0",
    "prettier": "^3.0.3",
    "tslib": "^2.6.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vite": "^6.3.1",
    "vitest": "^3.1.1"
  },
  "peerDependencies": {
    "@vue/compiler-core": "^3.4.6",
    "vite": "^4 || ^5 || ^6 || ^7",
    "vue": ">= 3.2.13"
  },
  "optionalDependencies": {
    "pug-lexer": "^5.0.1",
    "pug-parser": "^6.0.0",
    "pug-source-gen": "^0.0.2",
    "pug-walk": "^2.0.0"
  }
}
